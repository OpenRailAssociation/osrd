                },
                "case_none": {
                    "type": "aspect_set",
                    "members": [
                        {
                            "aspect": "RED"
                        }
                    ]
                },
                "case_some": {
                    "type": "condition",
                    "if": {
                        "type": "or",
                        "exprs": [
                            {
                                "type": "route_has_state",
                                "route": {
                                    "type": "optional_match_ref",
                                    "match_name": "route"
                                },
                                "state": "CONFLICT"
                            },
                            {
                                "type": "route_has_state",
                                "route": {
                                    "type": "optional_match_ref",
                                    "match_name": "route"
                                },
                                "state": "OCCUPIED"
                            },
                            {
                                "type": "route_has_state",
                                "route": {
                                    "type": "optional_match_ref",
                                    "match_name": "route"
                                },
                                "state": "REQUESTED"
                            }
                        ]
                    },
                    "then": {
                        "type": "aspect_set",
                        "members": [
                            {
                                "aspect": "YELLOW"
                            }
                        ]
                    },
                    "else": {
                        "type": "aspect_set",
                        "members": [
                            {
                                "aspect": "GREEN"
                            }
                        ]
                    }
                }
            }
        }
