  "scripts": {
    "compile": "tsc -b",
    "start": "exec scripts/start-dev.sh",
    "start-container": "import-meta-env-prepare -x .env.example && vite --host",
    "build": "vite build",
    "coverage-build": "VITE_COVERAGE=true vite build --mode development",
    "test": "import-meta-env-prepare -x .env.example && vitest",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --max-warnings 0",
    "lint-fix": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "lint-group-type": "eslint --ext .ts,.tsx,.js,.jsx src -f scripts/lint-formatter.js",
    "test-coverage": "import-meta-env-prepare -x .env.example && vitest run --coverage",
    "generate-types": "exec scripts/generate-types.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-licenses": "license-checker-rseidelsohn --json --production --direct --excludePackagesStartingWith '@types' --excludePrivatePackages --customPath src/common/ReleaseInformations/json/licenseCustomFormat.json > src/common/ReleaseInformations/json/licenses.json",
    "e2e-tests": "yarn playwright test --reporter=line"
  },
  "browserslist": {
    "production": [
      "> 0.2% in FI",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
